{
  "manifest_version": 2,

  "name": "Tab change reload named Masamune",
  "description": "The first plugin of chrome from zhangliyuan.",
  "version": "1.0",
  "background": { "scripts": ["background.js"] },
  "icons": {
	"48": "icon.png",
	"128": "icon.png"
  },
  "permissions": [
    "tabs", 
    "http://*/*", 
    "https://*/*", 
    "notifications"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "background.html"
  }
}